<AnalysisConfig>
    <IOInfo Type="Real">
        <DataDir>data/Sn108Mult50_55/</DataDir>
        <RunFirst>2272</RunFirst>
        <RunLast>2500</RunLast>
    </IOInfo>
    <TaskList>
        <EventFilterTask>
            <MultiplicityMin>50</MultiplicityMin>
            <MultiplicityMax>100</MultiplicityMax>
            <MultiplicityDPOCA>20</MultiplicityDPOCA>
        </EventFilterTask>
    
        <PIDProbTask Type="Bay">
            <MetaFile>Prior/Meta_Sn108KanekoMult50.root</MetaFile>
            <PIDFit>PIDFit/PIDSigma_Sn108KanekoMult50.root</PIDFit>
        </PIDProbTask>

        <PiProbTask>
            <GausFile>piAna/sliceFit.txt</GausFile>
        </PiProbTask>
    
        <TransformFrameTask>
            <TargetMass>112</TargetMass>
            <TargetThickness>0.8</TargetThickness>
